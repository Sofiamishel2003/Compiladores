/* Definición de parser */
/* INICIA Sección de TOKENS */
%token INT
%token SUMA
%token MULT
%token PARIZQ
%token PARDER
%token WS
IGNORE WS
/* FINALIZA Sección de TOKENS */
%%
/* INICIA Sección de PRODUCCIONES */
S:
    T SUMA S
  | T
;

T:
    INT MULT T
  | INT
  | PARIZQ S PARDER
;
/* FINALIZA Sección de PRODUCCIONES */

